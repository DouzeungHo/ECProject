doctype html
html
  head
    block head
      include header.pug
      
  body
    include navigate.pug
              

    header#header.mui--z1
      div.mui-appbar.mui--appbar-height
        table(width='100%')
          tr
            td
              div.mui-container-fluid
                a.sidedrawer-toggle.mui--visible-xs-inline-block.mui--visible-sm-inline-block.js-show-sidedrawer ☰
                a.sidedrawer-toggle.mui--hidden-xs.mui--hidden-sm.js-hide-sidedrawer ☰
                  a.mui--text-title(href='/')
                    button.mui-btn.mui-btn--primary 拼团游
            td(align='right')
              include none-user-bar.pug

    div#content-wrapper
      div.mui--appbar-height
      div#strange.mui-container-fluid
        block content

    footer#footer
      div.mui-container.mui--text-center
        br
        p Made with <a href="https://www.muicss.com")> MUICSS </a>